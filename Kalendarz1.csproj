    <EmbeddedResource Remove="Nowy folder2\**" />
    <EmbeddedResource Remove="Nowy folder3\**" />
    <None Remove="Kalendarz\**" />
    <None Remove="Menu\**" />
    <None Remove="Nowy folder2\**" />
