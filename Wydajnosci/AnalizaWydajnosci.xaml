<Window x:Class="Kalendarz1.AnalizaWydajnosciKrojenia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="🔪 Analiza Wydajności Krojenia - Kontrola Przepływu Produkcji" 
        Height="950" Width="1500"
        WindowStartupLocation="CenterScreen"
        Background="#F0F3F7">

    <Window.Resources>
        <!-- Style dla przycisków -->
        <Style x:Key="ButtonStyle" TargetType="Button">
            <Setter Property="Background" Value="#2C3E50"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontWeight" Value="SemiBold"/>
            <Setter Property="FontSize" Value="12"/>
            <Setter Property="Padding" Value="15,8"/>
            <Setter Property="Margin" Value="5"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="Cursor" Value="Hand"/>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#34495E"/>
                </Trigger>
            </Style.Triggers>
        </Style>

        <!-- Style dla kart metrycznych -->
        <Style x:Key="MetricCard" TargetType="Border">
            <Setter Property="Background" Value="White"/>
            <Setter Property="CornerRadius" Value="10"/>
            <Setter Property="Margin" Value="5"/>
            <Setter Property="Padding" Value="20"/>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Effect">
                        <Setter.Value>
                            <DropShadowEffect ShadowDepth="2" BlurRadius="10" Opacity="0.2"/>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </Style.Triggers>
        </Style>

        <!-- Style dla wartości pozytywnych/negatywnych -->
        <Style x:Key="PositiveValue" TargetType="TextBlock">
            <Setter Property="Foreground" Value="#27AE60"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="FontSize" Value="24"/>
        </Style>

        <Style x:Key="NegativeValue" TargetType="TextBlock">
            <Setter Property="Foreground" Value="#E74C3C"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="FontSize" Value="24"/>
        </Style>

        <Style x:Key="NeutralValue" TargetType="TextBlock">
            <Setter Property="Foreground" Value="#3498DB"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="FontSize" Value="24"/>
        </Style>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <!-- Nagłówek -->
        <Border Grid.Row="0" Background="#1ABC9C" Padding="25">
            <Grid>
                <StackPanel>
                    <TextBlock Text="🔪 ANALIZA WYDAJNOŚCI KROJENIA" 
                               FontSize="24" 
                               FontWeight="Bold" 
                               Foreground="White"/>
                    <TextBlock Text="Kontrola przepływu: Tuszka B → Krojenie → Elementy + Podroby | Wykrywanie niedogodności produkcyjnych" 
                               Foreground="White"
                               Opacity="0.95"
                               FontSize="13"
                               Margin="0,5,0,0"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center">
                    <TextBlock x:Name="txtDataCzas" 
                               Foreground="White"
                               FontSize="14"
                               Margin="0,0,20,0"/>
                    <Border Background="White" CornerRadius="15" Padding="10,5" Opacity="0.2">
                        <TextBlock x:Name="txtStatusBazy" 
                                   Text="📊 Baza: HANDEL"
                                   Foreground="White"
                                   FontWeight="Bold"/>
                    </Border>
                </StackPanel>
            </Grid>
        </Border>

        <!-- Panel filtrów -->
        <Border Grid.Row="1" Background="White" Margin="10" CornerRadius="8" Padding="15">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="150"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="150"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="150"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <TextBlock Grid.Column="0" 
                           Text="📅 Od:" 
                           VerticalAlignment="Center"
                           FontWeight="SemiBold"
                           Margin="0,0,10,0"/>
                <DatePicker Grid.Column="1" 
                            x:Name="dpDataOd"
                            VerticalAlignment="Center"/>

                <TextBlock Grid.Column="2" 
                           Text="Do:" 
                           VerticalAlignment="Center"
                           FontWeight="SemiBold"
                           Margin="20,0,10,0"/>
                <DatePicker Grid.Column="3" 
                            x:Name="dpDataDo"
                            VerticalAlignment="Center"/>

                <TextBlock Grid.Column="4" 
                           Text="Próg [%]:" 
                           VerticalAlignment="Center"
                           FontWeight="SemiBold"
                           Margin="20,0,10,0"
                           ToolTip="Dopuszczalna różnica wydajności"/>
                <ComboBox Grid.Column="5" 
                          x:Name="cbProg"
                          VerticalAlignment="Center">
                    <ComboBoxItem Content="± 1%" />
                    <ComboBoxItem Content="± 2%" IsSelected="True"/>
                    <ComboBoxItem Content="± 3%" />
                    <ComboBoxItem Content="± 5%" />
                    <ComboBoxItem Content="± 10%" />
                </ComboBox>

                <CheckBox Grid.Column="6" 
                          x:Name="chkTylkoProblemy"
                          Content="🔴 Tylko dni z problemami"
                          VerticalAlignment="Center"
                          FontWeight="SemiBold"
                          Margin="30,0,0,0"/>

                <StackPanel Grid.Column="7" Orientation="Horizontal">
                    <Button Content="🔍 Analizuj" 
                            Style="{StaticResource ButtonStyle}"
                            Background="#3498DB"
                            Click="BtnAnalizuj_Click"/>
                    <Button Content="📊 Raport" 
                            Style="{StaticResource ButtonStyle}"
                            Background="#9B59B6"
                            Click="BtnRaport_Click"/>
                </StackPanel>
            </Grid>
        </Border>

        <!-- Karty z metrykami KPI -->
        <Grid Grid.Row="2" Margin="10,0,10,10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!-- Karta: Tuszka B (wejście) -->
            <Border Grid.Column="0" Style="{StaticResource MetricCard}">
                <StackPanel>
                    <Grid>
                        <TextBlock Text="📥 TUSZKA B" FontWeight="Bold" Foreground="#2C3E50"/>
                        <TextBlock Text="(do krojenia)" 
                                   HorizontalAlignment="Right"
                                   FontSize="9"
                                   Foreground="#95A5A6"/>
                    </Grid>
                    <Separator Margin="0,8,0,8" Background="#ECF0F1"/>
                    <TextBlock Text="Wejście [kg]:" FontSize="11" Foreground="#7F8C8D"/>
                    <TextBlock x:Name="txtTuszkaBWejscie" Style="{StaticResource NeutralValue}"/>
                    <TextBlock Text="Średnio/dzień:" FontSize="11" Foreground="#7F8C8D" Margin="0,8,0,0"/>
                    <TextBlock x:Name="txtTuszkaBSrednio" FontSize="16" FontWeight="Bold"/>
                </StackPanel>
            </Border>

            <!-- Karta: Elementy (wyjście) -->
            <Border Grid.Column="1" Style="{StaticResource MetricCard}">
                <StackPanel>
                    <Grid>
                        <TextBlock Text="📦 ELEMENTY" FontWeight="Bold" Foreground="#2C3E50"/>
                        <TextBlock Text="(z krojenia)" 
                                   HorizontalAlignment="Right"
                                   FontSize="9"
                                   Foreground="#95A5A6"/>
                    </Grid>
                    <Separator Margin="0,8,0,8" Background="#ECF0F1"/>
                    <TextBlock Text="Wyjście [kg]:" FontSize="11" Foreground="#7F8C8D"/>
                    <TextBlock x:Name="txtElementyWyjscie" Style="{StaticResource NeutralValue}"/>
                    <TextBlock Text="Liczba pozycji:" FontSize="11" Foreground="#7F8C8D" Margin="0,8,0,0"/>
                    <TextBlock x:Name="txtElementyPozycje" FontSize="16" FontWeight="Bold"/>
                </StackPanel>
            </Border>

            <!-- Karta: Podroby -->
            <Border Grid.Column="2" Style="{StaticResource MetricCard}">
                <StackPanel>
                    <Grid>
                        <TextBlock Text="🦴 PODROBY" FontWeight="Bold" Foreground="#2C3E50"/>
                        <TextBlock Text="(sPWU)" 
                                   HorizontalAlignment="Right"
                                   FontSize="9"
                                   Foreground="#95A5A6"/>
                    </Grid>
                    <Separator Margin="0,8,0,8" Background="#ECF0F1"/>
                    <TextBlock Text="Ilość [kg]:" FontSize="11" Foreground="#7F8C8D"/>
                    <TextBlock x:Name="txtPodrobyIlosc" Style="{StaticResource NeutralValue}" Foreground="#E67E22"/>
                    <TextBlock Text="Typy:" FontSize="11" Foreground="#7F8C8D" Margin="0,8,0,0"/>
                    <TextBlock x:Name="txtPodrobyTypy" FontSize="12" TextWrapping="Wrap"/>
                </StackPanel>
            </Border>

            <!-- Karta: Wydajność -->
            <Border Grid.Column="3" Style="{StaticResource MetricCard}" BorderBrush="#3498DB" BorderThickness="2">
                <StackPanel>
                    <Grid>
                        <TextBlock Text="📈 WYDAJNOŚĆ" FontWeight="Bold" Foreground="#3498DB"/>
                        <TextBlock x:Name="txtWydajnoscStatus" 
                                   Text="✓"
                                   HorizontalAlignment="Right"
                                   FontSize="16"
                                   Foreground="#27AE60"/>
                    </Grid>
                    <Separator Margin="0,8,0,8" Background="#3498DB" Opacity="0.3"/>
                    <TextBlock Text="Średnia [%]:" FontSize="11" Foreground="#7F8C8D"/>
                    <TextBlock x:Name="txtWydajnoscSrednia" FontSize="28" FontWeight="Bold"/>
                    <TextBlock Text="Zakres:" FontSize="11" Foreground="#7F8C8D" Margin="0,8,0,0"/>
                    <TextBlock x:Name="txtWydajnoscZakres" FontSize="12"/>
                </StackPanel>
            </Border>

            <!-- Karta: Niedogodności -->
            <Border Grid.Column="4" Style="{StaticResource MetricCard}">
                <StackPanel>
                    <Grid>
                        <TextBlock Text="⚠️ ALERTY" FontWeight="Bold" Foreground="#E74C3C"/>
                        <TextBlock x:Name="txtAlertyBadge" 
                                   Text="0"
                                   HorizontalAlignment="Right"
                                   Background="#E74C3C"
                                   Foreground="White"
                                   Padding="6,2"
                                   FontWeight="Bold"/>
                    </Grid>
                    <Separator Margin="0,8,0,8" Background="#E74C3C" Opacity="0.3"/>
                    <TextBlock Text="Różnica [kg]:" FontSize="11" Foreground="#7F8C8D"/>
                    <TextBlock x:Name="txtRoznicaKg" Style="{StaticResource NegativeValue}"/>
                    <TextBlock Text="Dni z problemem:" FontSize="11" Foreground="#7F8C8D" Margin="0,8,0,0"/>
                    <TextBlock x:Name="txtDniZProblemem" FontSize="16" FontWeight="Bold" Foreground="#E74C3C"/>
                </StackPanel>
            </Border>
        </Grid>

        <!-- Wykres trendu -->
        <Border Grid.Row="3" Background="White" Margin="10,0,10,10" CornerRadius="8" Height="150">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                
                <Border Grid.Row="0" Background="#ECF0F1" Padding="10,8" CornerRadius="8,8,0,0">
                    <TextBlock Text="📊 TREND WYDAJNOŚCI W CZASIE" 
                               FontWeight="Bold" 
                               FontSize="12"/>
                </Border>
                
                <Canvas Grid.Row="1" x:Name="canvasWykres" Margin="10">
                    <!-- Tu będzie rysowany wykres -->
                </Canvas>
            </Grid>
        </Border>

        <!-- Tabele szczegółowe -->
        <TabControl Grid.Row="4" Margin="10,0,10,10">
            <!-- Tab: Analiza dzienna -->
            <TabItem Header="📅 Analiza dzienna">
                <DataGrid x:Name="dgAnalizaDzienna" 
                          AutoGenerateColumns="False"
                          CanUserAddRows="False"
                          HeadersVisibility="Column"
                          GridLinesVisibility="All"
                          AlternatingRowBackground="#F8F9FA"
                          RowHeight="40">
                    <DataGrid.Resources>
                        <Style TargetType="DataGridRow">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding CzyProblem}" Value="True">
                                    <Setter Property="Background" Value="#FFE5E5"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Data" 
                                            Binding="{Binding DataProdukcji, StringFormat=dd.MM.yyyy}" 
                                            Width="100"/>
                        <DataGridTextColumn Header="Dzień tyg." 
                                            Binding="{Binding DzienTygodnia}" 
                                            Width="80"/>
                        <DataGridTextColumn Header="Tuszka B [kg]" 
                                            Binding="{Binding TuszkaBKg, StringFormat=N2}" 
                                            Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                    <Setter Property="FontWeight" Value="Bold"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Elementy [kg]" 
                                            Binding="{Binding ElementyKg, StringFormat=N2}" 
                                            Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                    <Setter Property="FontWeight" Value="Bold"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Podroby [kg]" 
                                            Binding="{Binding PodrobyKg, StringFormat=N2}" 
                                            Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Suma wyj. [kg]" 
                                            Binding="{Binding SumaWyjscie, StringFormat=N2}" 
                                            Width="110">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                    <Setter Property="FontWeight" Value="Bold"/>
                                    <Setter Property="Foreground" Value="#2980B9"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Wydajność %" 
                                            Binding="{Binding WydajnoscProcent, StringFormat=N1}" 
                                            Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="FontWeight" Value="Bold"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding CzyProblem}" Value="True">
                                            <Setter Property="Foreground" Value="#E74C3C"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding CzyProblem}" Value="False">
                                            <Setter Property="Foreground" Value="#27AE60"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Różnica [kg]" 
                                            Binding="{Binding RoznicaKg, StringFormat=N2}" 
                                            Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                    <Setter Property="FontWeight" Value="Bold"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding CzyUjemna}" Value="True">
                                            <Setter Property="Foreground" Value="#E74C3C"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Status" 
                                            Binding="{Binding Status}" 
                                            Width="120">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="FontWeight" Value="Bold"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Uwagi" 
                                            Binding="{Binding Uwagi}" 
                                            Width="*"/>
                    </DataGrid.Columns>
                </DataGrid>
            </TabItem>

            <!-- Tab: Szczegóły elementów -->
            <TabItem Header="📦 Szczegóły elementów">
                <DataGrid x:Name="dgSzczegolyElementy" 
                          AutoGenerateColumns="False"
                          CanUserAddRows="False"
                          HeadersVisibility="Column"
                          GridLinesVisibility="All"
                          AlternatingRowBackground="#F8F9FA">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Data" 
                                            Binding="{Binding DataProdukcji, StringFormat=dd.MM.yyyy}" 
                                            Width="100"/>
                        <DataGridTextColumn Header="Kod produktu" 
                                            Binding="{Binding KodProduktu}" 
                                            Width="150"/>
                        <DataGridTextColumn Header="Nazwa" 
                                            Binding="{Binding NazwaProduktu}" 
                                            Width="300"/>
                        <DataGridTextColumn Header="Kategoria" 
                                            Binding="{Binding Kategoria}" 
                                            Width="100"/>
                        <DataGridTextColumn Header="Przychód [kg]" 
                                            Binding="{Binding Przychod, StringFormat=N2}" 
                                            Width="100">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Do krojenia [kg]" 
                                            Binding="{Binding Krojenie, StringFormat=N2}" 
                                            Width="110">
                            <DataGridTextColumn.ElementStyle>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Right"/>
                                    <Setter Property="Padding" Value="0,0,5,0"/>
                                </Style>
                            </DataGridTextColumn.ElementStyle>
                        </DataGridTextColumn>
                        <DataGridTextColumn Header="Seria" 
                                            Binding="{Binding Seria}" 
                                            Width="80"/>
                        <DataGridTextColumn Header="Magazyn" 
                                            Binding="{Binding Magazyn}" 
                                            Width="*"/>
                    </DataGrid.Columns>
                </DataGrid>
            </TabItem>

            <!-- Tab: TOP produkty -->
            <TabItem Header="🏆 TOP produkty">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    
                    <DataGrid Grid.Column="0" 
                              x:Name="dgTopElementy" 
                              AutoGenerateColumns="False"
                              CanUserAddRows="False"
                              Margin="5">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="TOP Elementy (wg ilości)" 
                                                Binding="{Binding Nazwa}" 
                                                Width="*"/>
                            <DataGridTextColumn Header="Suma [kg]" 
                                                Binding="{Binding Ilosc, StringFormat=N2}" 
                                                Width="100">
                                <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="HorizontalAlignment" Value="Right"/>
                                        <Setter Property="FontWeight" Value="Bold"/>
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                    
                    <DataGrid Grid.Column="1" 
                              x:Name="dgTopPodroby" 
                              AutoGenerateColumns="False"
                              CanUserAddRows="False"
                              Margin="5">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="TOP Podroby" 
                                                Binding="{Binding Nazwa}" 
                                                Width="*"/>
                            <DataGridTextColumn Header="Suma [kg]" 
                                                Binding="{Binding Ilosc, StringFormat=N2}" 
                                                Width="100">
                                <DataGridTextColumn.ElementStyle>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="HorizontalAlignment" Value="Right"/>
                                        <Setter Property="FontWeight" Value="Bold"/>
                                    </Style>
                                </DataGridTextColumn.ElementStyle>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </Grid>
            </TabItem>
        </TabControl>

        <!-- Stopka -->
        <Border Grid.Row="5" Background="#2C3E50" Padding="15">
            <Grid>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="💡 " 
                               Foreground="White" 
                               FontSize="14"
                               VerticalAlignment="Center"/>
                    <TextBlock x:Name="txtPodsumowanie" 
                               Foreground="White" 
                               FontSize="12"
                               VerticalAlignment="Center"/>
                </StackPanel>
                
                <StackPanel Orientation="Horizontal" 
                            HorizontalAlignment="Right">
                    <Button Content="🔄 Automatyczna analiza" 
                            Style="{StaticResource ButtonStyle}"
                            Background="#F39C12"
                            Click="BtnAutoAnaliza_Click"
                            ToolTip="Włącz automatyczne odświeżanie co 5 minut"/>
                    <Button Content="⚙ Ustawienia norm" 
                            Style="{StaticResource ButtonStyle}"
                            Background="#95A5A6"
                            Click="BtnUstawieniaNorm_Click"/>
                    <Button Content="✕ Zamknij" 
                            Style="{StaticResource ButtonStyle}"
                            Background="#E74C3C"
                            Click="BtnZamknij_Click"/>
                </StackPanel>
            </Grid>
        </Border>
    </Grid>
</Window>
